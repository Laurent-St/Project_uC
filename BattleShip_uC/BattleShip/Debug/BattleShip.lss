00005d 9a5b                      SBI PORTD,3
                                 
                                 ;%%%%%%%%%%%%%%%%%%%%%%%%% POINTER FOR 1ST BUFFER: : LOADED STARTING AT DATA ADDRESS 0x0100 %%%%%%%%
00005e e4ec                      LDI ZL,low(initbuffer<<1) ;pointer to values in the program memory
00005f e0f6                      LDI ZH,high(initbuffer<<1)
                                 
000060 e0a0                      LDI XL,0x00 ;pointer to values in the data memory
000061 e0b1                      LDI XH,0x01
                                 ;--> thus X=XH+XL=0x0100
                                 
